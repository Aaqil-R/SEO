<section class="video-comp">
	<div class="container video-component">
		<div class="heading">
			<h2>{{component.heading}}</h2>
		</div>
		<div class="video">
	          <div class="fullwidth__video" video-url="{{component.video}}"></div>
	          <div id="play-button" class="c-hero_playbutton"></div>
	        </div>
		<div class="quote">
			<p>
			{{component.quote}}
			</p>
		</div>
	</div>
</section>