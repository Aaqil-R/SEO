<section class="c-news-header">
  <div class="container">
  	<div class="c-news-header__breadcrumb">
  		<a href="{{site_url}}" class="parent">{{site_name}}</a> <p> > </p> <a href="{{options['news_&_events_landing_page']}}" class="current c-rte__link">NEWS & EVENTS</a>
  	</div>
    <h1>{{post.title}}</h1> {% if component.header_medium %}
    <p><span class="c-news-header__medium">{{component.header_medium}}</span> {{component.header_text}}</p>
    {% endif %}
  </div>
  <img src="{{Image(component['header_image']).src}}">
</section>
