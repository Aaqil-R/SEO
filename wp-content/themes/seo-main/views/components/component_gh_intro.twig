      <div class="c-gh-intro__content">
        <p>{{component.intro_text}}</p>
        <a href="{{component.button.url}}" class="c-uie__calltoaction">
        	{% if component.button.title %}
	        	{{component.button.title}}
        	{% else %}
					  {{component.button.url}}
        	{% endif %}
        </a>
      </div>
    </div>
  </div>
</section>
